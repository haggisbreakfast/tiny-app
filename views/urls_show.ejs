  <script src="main.js"></script>
</head>
<h1>
  URLS
</h1>
<!-- update operation - update existing shortened urls  -->
<form method="POST" action="/urls/<%=shortURL%>/update">
  <!-- text input for update button -->
  <!-- longURL: urlDatabase[req.params.id] -->
  <input type="text" value="<%= longURL %>" name="newURL" />
  <button type="submit" class="btn btn-primary">Update</button>
</form>
<ul>
  <a href=>
    <%=longURL%>
  </a>
</ul>

